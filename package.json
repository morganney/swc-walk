{
  "name": "swc-walk",
  "version": "1.0.0",
  "description": "Walk an AST from SWC and visit each node type.",
  "main": "dist/esm/walk.js",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/walk.d.ts",
        "default": "./dist/esm/walk.js"
      },
      "require": {
        "types": "./dist/cjs/walk.d.cts",
        "default": "./dist/cjs/walk.cjs"
      },
      "default": "./dist/esm/walk.js"
    },
    "./baseVisitor": {
      "import": {
        "types": "./dist/esm/baseVisitor.d.ts",
        "default": "./dist/esm/baseVisitor.js"
      },
      "require": {
        "types": "./dist/cjs/baseVisistor.d.cts",
        "default": "./dist/cjs/baseVisitor.cjs"
      },
      "default": "./dist/esm/baseVisitor.js"
    },
    "./package.json": "./package.json"
  },
  "engines": {
    "node": ">=20.2.0"
  },
  "engineStrict": true,
  "scripts": {
    "prettier": "prettier -w .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "build": "duel --dirs",
    "test": "c8 --reporter=text --reporter=text-summary --reporter=lcov tsx --test --test-reporter=spec test/*.test.ts",
    "prepack": "npm run build"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "swc",
    "ast",
    "walk",
    "visitor",
    "traverse",
    "acorn-walk"
  ],
  "author": "Morgan Ney <morganney@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/morganney/swc-walk.git"
  },
  "bugs": {
    "url": "https://github.com/morganney/swc-walk/issues"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@knighted/duel": "^2.1.5",
    "@knighted/dump": "^1.0.3",
    "@stylistic/eslint-plugin-js": "^2.1.0",
    "@swc/core": "^1.13.5",
    "@swc/types": "^0.1.24",
    "@types/node": "^24.3.0",
    "c8": "^10.1.3",
    "eslint": "^9.34.0",
    "eslint-plugin-n": "^17.21.3",
    "prettier": "^3.6.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0"
  },
  "dependencies": {
    "acorn-walk": "^8.3.4"
  },
  "prettier": {
    "arrowParens": "avoid",
    "printWidth": 119,
    "semi": false,
    "singleQuote": true
  }
}
